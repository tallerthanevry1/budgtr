<!DOCTYPE html>
<html>
    <%- include("./partials/head.ejs") %>
    <body>
        <h1>Main Menu</h1>
        <h2 class="<%= className %> "> Bank Total $<%= bankTotal %> </h1>
        <nav>
            <a href="/budgets/new">Create budget</a>
        </nav>
        <ul>
            <% for(let i = 0; i < allBudgets.length; i++) { %>
                <li>
                    <%=allBudgets[i].date %>,   <a href="/budgets/<%=i%>">   <%=allBudgets[i].name %>,</a>   <%=allBudgets[i].amount %>$
                    <form action="/budgets/<%= i %>?_method=DELETE" method="POST">
                        <input type="submit" value="Delete">
                    </form>
                </li>
            <% } %>
        </ul>
        
    </body>
</html>